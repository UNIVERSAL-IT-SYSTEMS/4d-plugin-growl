<?xml version="1.0" encoding="UTF-8" standalone="no" ?><ex4D:plugin xmlns:ex4D="http://www.4d.com/plugin">  <!--Growl II-->  <ex4D:documentation name="Growl II" plugin-id="20000">    <ex4D:plugin-description>      <!-- write documentation for Growl II here -->	  <p>This plugin is a 4D implementation of the <a href="http://growl.info/">Growl</a> 1.3 SDK. It focuses on simply posting notifications, using the new &quot;mist&quot; feature, which allows applications to post Growl notifications even if Growl is not installed on the local computer.</p>	      </ex4D:plugin-description>    <ex4D:commands>      <ex4D:command-group command-group-id="1" name="1.3 SDK">        <ex4D:command command-id="1" name="Growl Set notification method">          <ex4D:command-description>            <!-- write documentation for Growl Set notification method here -->			<p>Define the project method that should be called when a notification is clicked or timed out.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="method" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The method name.</p>			          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>1, on success.</p>			          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="2" name="Growl SET MIST ENABLED">          <ex4D:command-description>            <!-- write documentation for Growl SET MIST ENABLED here -->			<p>Enables or disables mist. Notification with this plugin will not work if mist is disabled.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="enabled" type="L">            <!-- write documentation for $1 (LONGINT) here-->			<p>1 to enable, 0 to disable.</p>          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="3" name="Growl Get mist enabled">          <ex4D:command-description>            <!-- write documentation for Growl Get mist enabled here -->			<p>Examine whether mist is enabled or not.</p>          </ex4D:command-description>          <ex4D:return-value label="enabled" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>1 if enabled, 0 if disabled.</p>			          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="4" name="Growl POST NOTIFICATION">          <ex4D:command-description>            <!-- write documentation for Growl POST NOTIFICATION here -->			<p>Post a Growl notification.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="title" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>A title for this notification.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="description" type="T">            <!-- write documentation for $2 (TEXT) here-->			<p>A description of the notification, which should provide more detail but should not flood the user with information.</p>			          </ex4D:argument>          <ex4D:argument argument-id="3" label="notificationName" type="T">            <!-- write documentation for $3 (TEXT) here-->			<p>A name for this notification.</p>			          </ex4D:argument>          <ex4D:argument argument-id="4" label="iconData" type="O">            <!-- write documentation for $4 (BLOB) here-->			<p>The default 4D icon will be used if the BLOB size is 0.</p>			          </ex4D:argument>          <ex4D:argument argument-id="5" label="priority" type="L">            <!-- write documentation for $5 (LONGINT) here-->			<p>Pass a value between -2 and +2.</p>			          </ex4D:argument>          <ex4D:argument argument-id="6" label="isSticky" type="L">            <!-- write documentation for $6 (LONGINT) here-->			<p>1 to make the notification sticky.</p>			          </ex4D:argument>          <ex4D:argument argument-id="7" label="clickContext" type="T">            <!-- write documentation for $7 (TEXT) here-->			<p>Pass an empty string if you do not care about call back. The context will be apparent in the process name of the call back method when fired.</p>			          </ex4D:argument>          <ex4D:argument argument-id="8" label="identifier" type="T">            <!-- write documentation for $8 (TEXT) here-->			<p>Notification of the same identifier are grouped together.</p>			          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="5" name="Growl Get notification method">          <ex4D:command-description>            <!-- write documentation for Growl Get notification method here -->			<p>Returns the name of the call back method.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="method" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The name of the call back method.</p>			          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>1, on success.</p>			          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>    </ex4D:commands>  </ex4D:documentation></ex4D:plugin>